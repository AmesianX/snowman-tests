shell = /bin/sh -c
diff = diff
grep = grep
run = run.py
tee = tee
decompiler = nocode
timeout = 300
exit_code = 0

rule decompile
    command = $shell "$run --stdout $stdout --stderr $stderr --timeout $timeout --exit-code $exit_code $decompiler $in"
    description = Decompile $in

rule check_equal
    command = $shell "$diff -u $in | $tee $out"
    description = Diff $in

rule check_matches
    command = $shell "$grep -f $in > $out"
    description = Grep $in
